{
  "cleanUrls": true,
  "trailingSlash": false,
  "routes": [
    { "src": "^/$", "dest": "/index.html" },

    { "src": "^/home/?$", "dest": "/index.html" },

    { "src": "^/solutions/?$", "dest": "/solutions.html" },
    { "src": "^/industries/?$", "dest": "/industries.html" },
    { "src": "^/resources/?$", "dest": "/resources.html" },
    { "src": "^/why-us/?$", "dest": "/why-us.html" },

    { "src": "^/contact/?$", "dest": "/contact.html" },

    { "src": "^/passport/(.*)$", "dest": "/passport/$1.html" },

    { "src": "^/assets/(.*)$", "dest": "/assets/$1" }
  ]
}
